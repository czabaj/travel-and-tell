{
  "dependencies": {
    "htm": "^3.0.3",
    "preact": "^10.3.1",
    "ramda": "^0.27.0",
    "ramda-adjunct": "^2.25.0"
  },
  "devDependencies": {
    "@fullhuman/postcss-purgecss": "^2.0.6",
    "concurrently": "^5.1.0",
    "postcss-cli": "^7.1.0",
    "prettier": "1.19.1",
    "snowpack": "^1.2.0",
