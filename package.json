  },
  "scripts": {
    "build": "NODE_ENV=production snowpack --dest wwwroot/web_modules --optimize && yarn build:css -m",
    "build:css": "postcss index.css -o wwwroot/index.css",
    "dev": "concurrently \"yarn build:css -w\" \"npx servor --reload wwwroot\"",
